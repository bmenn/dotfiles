[credential]
	help = cache
[http]
	sslVerify = false
[core]
	excludesfile = ~/.gitignore
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	ph = push
        pl = pull
	a = add
	d = diff
	ds = diff --staged
        f = fetch

[include]
        path = ~/.gitconfig_local
